# Camoufox Connector Configuration
# Copy this file to .env and customize as needed

# Operating mode: "single" or "pool"
MODE=single

# Number of browser instances (only used in pool mode)
POOL_SIZE=3

# HTTP API port for health checks and management
API_PORT=8080

# Starting port for browser WebSocket endpoints
WS_PORT_START=9222

# Browser configuration
HEADLESS=true
GEOIP=true
HUMANIZE=true
BLOCK_IMAGES=false

# Proxy configuration (optional)
# PROXY=http://user:pass@host:port

# Debug mode
DEBUG=false
